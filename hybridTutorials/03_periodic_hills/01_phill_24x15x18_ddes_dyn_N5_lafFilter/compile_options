OPTION(FLEXI_EXACT_MASSMATRIX "No help, variable specified on the command line." OFF)
OPTION(FLEXI_FV "No help, variable specified on the command line." OFF)
OPTION(FLEXI_FV_RECONSTRUCTION "No help, variable specified on the command line." OFF)
SET(FLEXI_LIFTING "br2" CACHE STRING "No help, variable specified on the command line.")
SET(FLEXI_NODETYPE "GAUSS" CACHE STRING "No help, variable specified on the command line.")
OPTION(FLEXI_PARABOLIC "No help, variable specified on the command line." ON)
OPTION(FLEXI_PERFORMANCE "Enable performance optimizations" ON)
OPTION(FLEXI_PERFORMANCE_IPO "Enable Interprocedural Optimization for build (if available)." ON)
OPTION(FLEXI_PERFORMANCE_PGO "Enable profile-guided optimization (Only GNU Compiler supported)" OFF)
SET(FLEXI_POLYNOMIAL_DEGREE "N" CACHE STRING "No help, variable specified on the command line.")
OPTION(FLEXI_PP_LIMITER "No help, variable specified on the command line." OFF)
OPTION(FLEXI_SPLIT_DG "No help, variable specified on the command line." OFF)
