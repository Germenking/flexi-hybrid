OPTION(FLEXI_UNITTESTS "No help, variable specified on the command line." ON)
SET(FLEXI_VISCOSITY "constant" CACHE STRING "No help, variable specified on the command line.")
OPTION(LIBS_BUILD_HDF5 "No help, variable specified on the command line." OFF)
OPTION(POSTI_SWAPMESH "No help, variable specified on the command line." ON)
