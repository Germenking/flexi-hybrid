{[( CMAKE )]}
OPTION(FLEXI_2D "Enable two dimensional computation" ON)
OPTION(FLEXI_EDDYVISCOSITY "Enable eddy viscosity modells" OFF)
SET(FLEXI_EOS "idealgas" CACHE STRING "Used equation of state (idealgas/table)")
SET(FLEXI_EQNSYSNAME "rans_sa" CACHE STRING "Used equation system (navierstokes/linearscalaradvection/rans_sa/rans_komega)")
OPTION(FLEXI_EXACT_MASSMATRIX "Build and invert exact mass Matrix" OFF)
OPTION(FLEXI_FV "Enable Finite Volume subcell shock capturing" OFF)
OPTION(FLEXI_FV_RECONSTRUCTION "Enable 2nd order reconstruction for FV" OFF)
SET(FLEXI_LIFTING "br1" CACHE STRING "Lifting method used for PARABOLIC")
SET(FLEXI_NODETYPE "GAUSS" CACHE STRING "Set of nodes used for representing the solution (GAUSS/GAUSS-LOBATTO).")
OPTION(FLEXI_PARABOLIC "Enable parabolic terms of equation-system" ON)
OPTION(FLEXI_PERFORMANCE "Enable performance optimizations" OFF)
OPTION(FLEXI_PERFORMANCE_IPO "Enable Interprocedural Optimization for build (if available)." ON)
SET(FLEXI_POLYNOMIAL_DEGREE "N" CACHE STRING "N: compile with variable polynomial degree. 1,2,3,...: compile with fixed polynomial degree for performance (untested).")
OPTION(FLEXI_PP_LIMITER "Enable Positivity Preserving Limiter" OFF)
OPTION(FLEXI_SPLIT_DG "Use split DG formulation" OFF)
SET(FLEXI_TESTCASE "default" CACHE STRING "Name of testcase which is used. Must be identical to directory name in testcase folder.")
OPTION(FLEXI_UNITTESTS "Enable unit tests after build" OFF)
SET(FLEXI_VISCOSITY "constant" CACHE STRING "Method to calculate viscosity (constant/sutherland/powerlaw)")
SET(CMAKE_BUILD_TYPE "Release" CACHE STRING "Choose the type of build, options are: None Debug Release RelWithDebInfo MinSizeRel ...")
OPTION(LIBS_BUILD_HDF5 "Compile and build HDF5 library" ON)
OPTION(LIBS_BUILD_MATH_LIB "Compile and build math library" OFF)
OPTION(LIBS_USE_MKL "Use system MKL libraries instead of system BLAS/LAPACK" OFF)
OPTION(LIBS_USE_MPI "Compile SINGLE or MPI version" ON)
OPTION(LIBS_USE_OPENMP "Enable OpenMP" OFF)
OPTION(LIBS_USE_PAPI "Use PAPI library to perform performance measurements (e.g. flop counts)." OFF)
{[( END USERBLOCK )]}
